<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
	http://www.springframework.org/schema/aop
	http://www.springframework.org/schema/aop/spring-aop-4.1.xsd
	http://www.springframework.org/schema/tx
	http://www.springframework.org/schema/tx/spring-tx-4.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.1.xsd
	http://www.springframework.org/schema/jee
	http://www.springframework.org/schema/jee/spring-jee-4.1.xsd">

	<!-- byName：为属性自动装配ID与该属性的名字相同的Bean -->
	<bean id="kenny" class="com.chapter3.autowired.demo4.Instrumentalist"/>

	<bean id="saxophone" class="com.chapter3.autowired.demo4.Saxophone" />

	<bean id="guitar" class="com.chapter3.autowired.demo4.Guitar" />

	<!-- 支持注解 -->
	<context:annotation-config />

</beans>